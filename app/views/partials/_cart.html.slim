.tags.bg-light ng-show="cart_visible" ng-init="cart_visible=true" style="border-right:1px solid #d4d8dc"
  .cleartags.fl-right.cursor
    img.spr.cleartags alt="" src="/i/blank.gif" ng-click="search.reset()"
  ul.tags
    li ng-repeat="item in search.items" class="{{ item.type }}" ng-class="{ active: item.active }" ng-click="item.toggle()"
      span = "{{ item.name }}"
